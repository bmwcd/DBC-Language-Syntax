{
	"name": "dbc",
	"displayName": "DBC",
	"description": "Provides VSCode support for DBC files",
	"repository": {
		"tpye": "git",
		"url": "https://github.com/lharri73/DBC-Language-Syntax"
	},
	"license": "GPL-2.0-only",
	"version": "1.0.2",
	"publisher": "lharri73",
	"engines": {
		"vscode": "^1.51.0"
	},
	"scripts": {
		"vscode:prepublish": "make"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"can",
		"dbc",
		"vector",
		"canbus"
	],
	"icon": "res/image.png",
	"contributes": {
		"languages": [
			{
				"id": "dbc",
				"aliases": [
					"DBC",
					"dbc"
				],
				"extensions": [
					".dbc"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "dbc",
				"scopeName": "source.dbc",
				"path": "./syntaxes/dbc.tmLanguage.json"
			}
		]
	},
	"devDependencies": {
		"js-yaml": "^3.14.0"
	},
