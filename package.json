{
    "name": "dbc",
    "displayName": "DBC Language Syntax",
    "description": "Provides VSCode support for CAN DBC files",
    "repository": "https://github.com/lharri73/DBC-Language-Syntax",
    "license": "GPL-2.0-only",
    "version": "1.0.1",
    "publisher": "lharri73",
    "engines": {
        "vscode": "^1.51.0"
    },
    "scripts": {
        "vscode:prepublish": "make"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dbc",
                "aliases": [
                    "DBC",
                    "dbc"
                ],
                "extensions": [
                    ".dbc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dbc",
                "scopeName": "source.dbc",
                "path": "./syntaxes/dbc.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^3.14.0"
    }
}
